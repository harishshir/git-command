git init <FolderName>
git init .
git checkout -b dev
git add .
git commit -m "message"
git remote add origin https://git-codecommit.us-east-2.amazonaws.com/v1/repos/sls-cicd-repo
git checkout dev
git push --set-upstream origin dev
git checkout master
git push --set-upstream origin master

Merge
-- set current branch
git checout master
git merge dev
